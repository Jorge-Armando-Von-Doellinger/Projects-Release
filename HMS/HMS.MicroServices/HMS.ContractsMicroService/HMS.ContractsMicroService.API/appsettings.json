{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ConnectionStrings": {
    "DefaultConnection": ""
  },
  "DefaultAppSettings": {
    "MongoDb": {
      "ConnectionString": "mongodb://localhost:27017"
    },
    "Consul": {
      "KvKey": "Hms/Micro-Services/Contracts-MicroService/Settings"
    },
    "RabbitMq": {
      "hostName": "localhost",
      "port": 5672,
      "queue": "contract",
      "exchange": "contracts",
      "typeExchange": "topic",
      "addKey": "contract.add",
      "deleteKey": "contract.remove",
      "updateKey": "contract.update",
      "responseKey": "response.contract"
    }
  },
  "AppName": "Contracts-MicroService",
  "AllowedHosts": "*"
}
